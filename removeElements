class Solution {
public:
    int removeElement(vector<int>& nums, int val) {

        int index=0;

        for(int i=0;i<nums.size();i++)
        {
          if(nums[i]!=val)
          
          {
            nums[index]=nums[i];
            index++;
          }

          
        }
        return index;
        
    }
};






## BRUTEFORCE --->

#include<bits/stdc++.h>
using namespace std;

int removeElements(vector<int>arr,int val)
{
    vector<int> temp;
    for(int i=0;i<arr.size();i++)
    {
        if(arr[i]!=val)
        {
            temp.push_back(arr[i]);
        }
    }
    
     for(int i=0;i<temp.size();i++)
     {
         arr[i]=temp[i];
     }
    return temp.size();
}

int main()
{
    int n;
    cout<<"enter the size of the array:";
    cin>>n;
    
    
    vector<int>arr(n);
    
    cout<<"\n enter the array elements:";
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    
    int val;
    cout << "\nEnter the value to remove: ";
    cin >> val;
    
    int result=removeElements(arr,val);
    
    cout << "\nNumber of elements after removal (k): " << result << endl;
    
}











## WITH BRACKETS,AND COMMA:
#include<bits/stdc++.h>
using namespace std;

int removeElements(vector<int>arr,int val)
{
    vector<int> temp;
    for(int i=0;i<arr.size();i++)
    {
        if(arr[i]!=val)
        {
            temp.push_back(arr[i]);
        }
    }
    
     for(int i=0;i<temp.size();i++)
     {
         arr[i]=temp[i];
     }
    return temp.size();
}

int main()
{
    string s;
    getline(cin,s);
    
    if (s.front() == '[' && s.back() == ']')
    {
        s = s.substr(1, s.size() - 2);
    }
    
    stringstream ss(s);
    
    vector<int>arr;
    
    int num;
    char ch;
    
    while(ss>>num)
    {
        arr.push_back(num);
        ss>>ch;
    }
    
    cout<<"\nthe output ";
    
    for(auto a:arr)
    {
        cout<<a<<" ";
    }
     int val;
    cout<<"\n The number should be removed ";
    cin>>val;
    
    int result= removeElements(arr,val);
    
    cout << "\nNumber of elements after removal (k): " << result << endl;
    
    cout<<"\n after removal the array will be ";
     for (int i = 0; i < result; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
    
    
    return 0;
}

